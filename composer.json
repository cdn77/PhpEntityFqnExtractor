{
    "name": "cdn77/entity-fqn-extractor",
    "type": "library",
    "description": "Entity FQN Extractor",
    "keywords": [
        "php"
    ],
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "require": {
        "php": "^8.2",
        "thecodingmachine/safe": "^2"
    },
    "require-dev": {
        "cdn77/coding-standard": "^7.0",
        "infection/infection": "^0.28.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^1.1",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpunit/phpunit": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Cdn77\\EntityFqnExtractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cdn77\\EntityFqnExtractor\\Tests\\": "tests/"
        }
    }
}
