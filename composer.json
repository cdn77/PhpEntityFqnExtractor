{
    "name": "cdn77/entity-fqn-extractor",
    "type": "library",
    "description": "Entity FQN Extractor",
    "keywords": [
        "php"
    ],
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php-64bit": "^7.4 || ^8.0",
        "thecodingmachine/safe": "^1"
    },
    "require-dev": {
        "cdn77/coding-standard": "^4.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "0.12.40",
        "phpstan/phpstan-phpunit": "0.12.16",
        "phpstan/phpstan-strict-rules": "0.12.4",
        "phpunit/phpunit": "^9.4",
        "psalm/plugin-phpunit": "^0.13.0",
        "simpod/php-coveralls-mirror": "^3.0",
        "vimeo/psalm": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Cdn77\\EntityFqnExtractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cdn77\\EntityFqnExtractor\\Tests\\": "tests/"
        }
    }
}
