{
    "name": "cdn77/entity-fqn-extractor",
    "type": "library",
    "description": "Entity FQN Extractor",
    "keywords": [
        "php"
    ],
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.0",
        "thecodingmachine/safe": "^1"
    },
    "require-dev": {
        "cdn77/coding-standard": "^5.0",
        "infection/infection": "^0.26.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^1.1",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpunit/phpunit": "^9.4",
        "psalm/plugin-phpunit": "^0.16.1",
        "roave/infection-static-analysis-plugin": "^1.8",
        "vimeo/psalm": "^4.12"
    },
    "autoload": {
        "psr-4": {
            "Cdn77\\EntityFqnExtractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cdn77\\EntityFqnExtractor\\Tests\\": "tests/"
        }
    }
}
